[CEStrings]
InstallDir=%CE1%\%AppName%
AppName="Pocket Heroes 1.03.01"

[Strings]
CompanyName="PH"

[Version]
Signature="$Windows NT$"
Provider=%CompanyName%
CESignature="$Windows CE$"

[CEDevice.SA2K3_ENG]
ProcessorType = 2577
VersionMin = 4.2            ; Specifies minimum version of Windows CE
VersionMax = 6.0            ; Specifies maximum version of Windows CE

[CEDevice.SA2K3_RUS]
ProcessorType = 2577
VersionMin = 4.2            ; Specifies minimum version of Windows CE
VersionMax = 6.0            ; Specifies maximum version of Windows CE

[Shortcut_Section]
Shortcut_name,Shortcut_type,Target_file,Standard_destination_path

[Shortcuts]
%AppName%,0,"PocketHeroes.exe"

[SourceDisksNames]
1=,"Binaries",,..\_release
2=,"Data",,..\_release\Data
3=,"Maps",,..\_release\Maps

;
; Install
;
[DefaultInstall]
