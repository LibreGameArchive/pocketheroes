CEShortcuts=Shortcuts		; Specifies common shortcut lists

[DefaultInstall.SA2K3_ENG]
CopyFiles=Files.SA2K3_ENG, Files.EngMaps

[DefaultInstall.SA2K3_RUS]
CopyFiles=Files.SA2K3_RUS, Files.RusMaps

